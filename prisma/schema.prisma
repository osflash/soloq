generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Guild {
  id            String   @id
  waitingRoomId String
  playingRoomId String
  verifiedId    String
  region        String
  createAt      DateTime @default(now())

  @@unique([id, waitingRoomId, playingRoomId, verifiedId])
  @@map("guilds")
}

model User {
  discordId String   @id @unique
  id        String   @db.VarChar(63)
  accountId String   @db.VarChar(56)
  name      String
  puuid     String   @db.VarChar(78)
  region    String
  createAt  DateTime @default(now())

  @@unique([discordId, id, accountId, puuid])
  @@map("users")
}
